  "containers": {
    "node-0": {
      "config": {
        "network": {
          "public": "disable",
          "hostname": "node-0",
          "ports": ["6379:6379", "16379:16379"]
        },
        "deploy": {
          "instances": 1
        },
        "runtime": {
          "environment_vars": {
            "ALLOW_EMPTY_PASSWORD":  "yes",
            "REDIS_NODES": "node-0 node-1 node-2 node-3 node-4 node-5",
            "REDIS_CLUSTER_SLEEP_BEFORE_DNS_LOOKUP": "30"
          }
        }
      },
      "name": "redis-node-0",
      "image": {
        "origin": {
          "type": "docker-registry",
          "details": {
            "url": "docker.io",
            "target": "bitnami/redis-cluster:latest"
          }
        }
      },
      "stateful": true,
      "volumes": [
        {
          "destination": "/bitnami/redis/data",
          "read_only": false,
          "local": {
            "max_size": "5GB"
          }
        }
      ]
    },
